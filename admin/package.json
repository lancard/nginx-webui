{
  "dependencies": {
    "chart.js": "^4.5.0",
    "chartjs-adapter-dayjs-4": "^1.0.4",
    "dayjs": "^1.11.13",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "jquery": "^3.7.1",
    "nginxbeautify": "^2.0.1",
    "session-file-store": "^1.5.0",
    "systeminformation": "^5.27.7"
  },
  "name": "nginx-webui",
  "version": "1.3.1",
  "type": "module",
  "description": "nginx webui",
  "main": "src/index.js",
  "scripts": {
    "build": "(npx postcss index.css -o static/dist/index.css) & (npx esbuild index.js --bundle --format=esm --target=esnext --outdir=static/dist --minify --sourcemap)",
    "watch": "(node --watch server.js) & (npx postcss --watch index.css -o static/dist/index.css) & (npx esbuild --watch index.js --bundle --format=esm --target=esnext --outdir=static/dist --minify --sourcemap)"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lancard/nginx-webui.git"
  },
  "keywords": [
    "nginx",
    "webui",
    "nginx-webui"
  ],
