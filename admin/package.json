  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lancard/nginx-webui/issues"
  },
  "homepage": "https://github.com/lancard/nginx-webui#readme",
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.11",
    "@tailwindcss/typography": "^0.5.16",
    "autoprefixer": "^10.4.21",
    "daisyui": "^5.0.46",
    "esbuild": "^0.25.8",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "tailwindcss": "^4.1.11"
  }
